{
  "name": "chart-test",
  "version": "1.0.0",
  "description": "use flow chart to create test plan",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p clear-dir && run-p copy-dep && run-p start-only",
    "start-only": "electron .",
    "clear-dir": "rimraf ./dep && make-dir ./dep && make-dir ./dep/frontend/ && make-dir ./dep/backend/",
    "copy-dep": "cpr ../frontend/dist/ ./dep/frontend/ && cpr ../backend/ ./dep/backend/"
  },
  "author": "rexlyc",
  "license": "MIT",
  "dependencies": {
    "cpr": "^3.0.1",
    "electron": "^24.1.2",
    "make-dir-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "python-shell": "^5.0.0",
    "rimraf": "^5.0.0",
    "tree-kill": "^1.2.2"
  }
}
